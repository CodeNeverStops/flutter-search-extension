{
    "manifest_version": 2,
    "name": "Flutter Search Extension",
    "description": "A handy browser extension to search official docs in the address bar (omnibox)",
    "version": "0.2",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "browser_action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
      "default_popup": "popup.html",
      "default_title": "A handy browser extension to search official docs in the address bar (omnibox)"
    },
    "omnibox": {
      "keyword": "ft"
    },
    "background": {
      "scripts": [
        "settings.js",
        "search.js",
        "search-index.js",
        "omnibox.js",
        "main.js"
      ]
    },
    "permissions": [
      "tabs"
    ]
  }